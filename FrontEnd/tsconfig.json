{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
        "paths": {
            "@/components/*": ["core/components/*"],
            "@/helper/*": ["core/helper/*"], 
            "@/layout/*": ["core/*"],            
            "@api/*": ["core/api/*"],
            "@/pages/*": ["pages/*"], 
            "@/*": ["core/*"],
        },
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "core/components/Navigation.jsx"],
  "exclude": ["node_modules"]
}
